  {%- endfor %}
    }
{%- else %}
    port_channel_fcoe_uplinks = {}
{%- endif %}
{%- if port_modes %}
    port_modes = [
      {
  {%- for k, v in port_modes.items() %}
    {%- if k == 'custom_mode' %}
        custom_mode = "{{ v }}"
    {%- elif k == 'port_list' %}
        port_list   = {{ v }}
    {%- elif k == 'slot_id' %}
        slot_id     = {{ v }}
    {%- endif %}
  {%- endfor %}
      }
    ]
{%- else %}
    port_modes = []
{%- endif %}
{%- if port_role_appliances %}
    port_role_appliances = {
  {%- for item in port_role_appliances %}
      "{{ loop.index }}" = {
