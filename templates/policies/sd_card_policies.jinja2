
  "{{ name }}" = {
    description        = "{{ descr }}"
    enable_os          = {{ partitions[0]['virtual_drives'][0]['enable']|lower }}
{%- if tags %}
    tags = [
  {%- for item in tags %}
      {
    {%- for k, v in item.items() %}
        {{ k }} = "{{ v }}",
    {%- endfor %}
      },
  {%- endfor %}
    ]
{%- else %}
    tags               = []
{%- endif %}
  }