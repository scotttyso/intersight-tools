
  "{{ name }}" = {
    admin_state  = "{{ admin_state }}"
    description  = "{{ descr }}"
    mode         = "{{ mode }}"
{%- if tags %}
    tags = [
  {%- for item in tags %}
      {
    {%- for k, v in item.items() %}
        {{ k }} = "{{ v }}",
    {%- endfor %}
      },
  {%- endfor %}
    ]
{%- else %}
    tags         = []
{%- endif %}
  }
